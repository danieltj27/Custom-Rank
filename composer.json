{
    "name": "danieltj/customrank",
    "type": "phpbb-extension",
    "description": "Add the option for a custom rank title.",
    "version": "1.0.0",
    "time": "2022-04-12",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Daniel James",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
    },
    "extra": {
        "display-name": "Custom Rank",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.7"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/customrank",
            "filename": "version_check",
            "ssl": true
        }
    }
}
